<div class="weather">
    <h1>Znajdź miejsce</h1>
    <div>Wolne miejsca znajdują się w:</div>
    <div>
        <table class="table table-dark">
            <thead>
            <tr>
                <th scope="col">Magazyn</th>
                <th scope="col">Rząd</th>
                <th scope="col">Regał</th>
                <th scope="col">Półka</th>
            </tr>
            </thead>
            <tbody>
            {% for shelf in freespace %}
            <tr>
                <td>{{ shelf['warehouse_id'] }}</td>
                <td>{{ shelf['row_id'] }}</td>
                <td>{{ shelf['rack_id'] }}</td>
                <td>{{ shelf['shelf_id'] }}</td>
            </tr>
            {% endfor %}
            </tbody>
        </table>

    </div>
</div>